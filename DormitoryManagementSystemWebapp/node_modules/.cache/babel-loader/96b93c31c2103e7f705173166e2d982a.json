ndow.sessionStorage.removeItem('atitle');\r\n            window.sessionStorage.removeItem('aid');\r\n            window.sessionStorage.removeItem('arouter');\r\n            // 历史页面回退\r\n            this.$router.go(-2);\r\n         }\r\n      },\r\n      mounted () {\r\n         // 当 会话缓存内容中 空值时,不显示详情页面,并重定向到home页面\r\n         if (this.content === '' || this.id === '' || this.router === '') {\r\n            this.$router.push('/home');\r\n         } else {\r\n            this.$router.push('/' + this.router);\r\n         }\r\n      }\r\n   }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n   .detail {\r\n      width: 100%;\r\n      border-top: 3px solid orange;\r\n      background-color: #fafafa;\r\n      min-width: 1080px;\r\n\r\n      .detail-header {\r\n         box-sizing: border-box;\r\n         -moz-box-sizing: border-box;\r\n         -webkit-box-sizing: border-box;\r\n         width: 100%;\r\n         padding: 24px;\r\n      }\r\n   }\r\n</style>\r\n"],"sourceRoot":"src/views/article-detail"}]}