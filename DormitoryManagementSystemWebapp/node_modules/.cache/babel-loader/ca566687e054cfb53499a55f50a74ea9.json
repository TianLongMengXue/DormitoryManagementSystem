\n         <!-- 首页标题区域 -->\r\n         <div class=\"home-title\">\r\n            <!-- 首页的主标题区域 -->\r\n            <div class=\"main-title\">\r\n               <!-- 首页主标题的内容及点击事件 -->\r\n               <a class=\"main-title-a\" @click.prevent=\"toControl\">\r\n                  &nbsp;学&nbsp;生&nbsp;宿&nbsp;舍&nbsp;\r\n                  <span style='color:#99CCFF'>管&nbsp;理&nbsp;</span>\r\n                  中&nbsp;&nbsp;\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-minus\"/>\r\n                  <i class=\"el-icon-arrow-right\"/>\r\n                  &nbsp;&nbsp;[&nbsp;&nbsp;\r\n                  <span style='color:green'>管&nbsp;理&nbsp;成&nbsp;功</span>\r\n                  &nbsp;&nbsp;]\r\n               </a>\r\n            </div>\r\n            <!-- 首页的副标题区域和内容 -->\r\n            <div class=\"subtitle\">\r\n               [&nbsp;&nbsp;\r\n               <span style='color:#FF9900'>美&nbsp;丽&nbsp;宿&nbsp;舍</span>\r\n               &nbsp;&nbsp;] &nbsp;&nbsp;\r\n               <span style='color:lightgreen'>Up&nbsp;Up&nbsp;Up</span>\r\n               &nbsp;&nbsp;!\r\n            </div>\r\n         </div>\r\n         <!-- 首页导航区域 -->\r\n         <div class=\"home-navigator\">\r\n            <ul>\r\n               <li @click=\"toControl\">控制台</li>\r\n               <li @click=\"logoutButton\">退出</li>\r\n            </ul>\r\n         </div>\r\n      </header>\r\n      <!-- 首页主体内容区域 -->\r\n      <main class=\"home-main\">\r\n         <!-- 文章区域 -->\r\n         <div class=\"article-panel\">\r\n            <!-- 文档导航菜单区域 -->\r\n            <el-menu mode=\"horizontal\" :default-active=\"activeIndex\" class=\"article-menu\">\r\n               <!-- 文章导航菜单内容 -->\r\n               <el-menu-item v-for=\"(item,index) in articleMenu\" :key=\"index\" :index=\"index + ''\" v-text=\"item\"></el-menu-item>\r\n            </el-menu>\r\n            <!-- 文章列表 -->\r\n            <div class=\"article-list\">\r\n               <!-- 文章 -->\r\n               <div class=\"article-list-item\" v-for=\"(item,index) in article\" :key=\"index\">\r\n                  <!-- 文章发布时间 -->\r\n                  <div class=\"article-date\">\r\n                     <span v-text=\"item.date\"></span>\r\n                  </div>\r\n                  <!-- 文章标题 -->\r\n                  <div class=\"article-title\">\r\n                     <a href=\"javascript:void(0);\" v-text=\"item.title\"></a>\r\n                  </div>\r\n                  <!-- 文章的描述 -->\r\n                  <div class=\"article-desc\" v-text=\"item.desc\"></div>\r\n                  <!-- 文章的发布 时间+部门 -->\r\n                  <div class=\"article-post\" v-text=\"item.post\"></div>\r\n               </div>\r\n            </div>\r\n            <!-- 分页查看 -->\r\n            <el-pagination\r\n               hide-on-single-page\r\n               background\r\n               class=\"article-pagination\"\r\n               layout=\"total, prev, pager, next\"\r\n               :total=\"pagination.pageTotal\"\r\n               :current-page.sync=\"pagination.currentPage\"\r\n               :page-size=\"pagination.pageSize\"\r\n               @current-change=\"handleCurrentChange\">\r\n            </el-pagination>\r\n         </div>\r\n         <!-- 首页信息区域 -->\r\n         <div class=\"info-panel\">\r\n            <div class=\"info-card\">\r\n               <!-- 个人信息的标题 -->\r\n               <h3>欢迎使用</h3>\r\n               <!-- 个人信息的内容 -->\r\n               <div class=\"info-box\">\r\n                  <ul>\r\n                     <li>学号：</li>\r\n                     <li>姓名：</li>\r\n                     <li>学院：</li>\r\n                  </ul>\r\n                  <ul>\r\n                     <li v-text=\"info.id\"></li>\r\n                     <li v-text=\"info.name\"></li>\r\n                     <li v-text=\"info.college\"></li>\r\n                  </ul>\r\n               </div>\r\n               <!-- 个人信息区域的按钮组 -->\r\n               <div class=\"buttons\">\r\n                  <el-button type=\"primary\" round @click=\"toControl\">控制台</el-button>\r\n                  <el-button type=\"info\" round @click=\"logoutButton\">退出</el-button>\r\n               </div>\r\n            </div>\r\n            <el-calendar class=\"info-calendar\" v-model=\"date\"></el-calendar>\r\n         </div>\r\n      </main>\r\n      <!-- 首页的底部区域 -->\r\n      <footer class=\"home-footer\">\r\n         Copyright © 2020 荆辞雪\r\n      </footer>\r\n   </div>\r\n</template>\r\n\r\n<script>\r\n   import $ from 'jquery'\r\n\r\n   export default {\r\n      name: 'Home',\r\n      data () {\r\n         return {\r\n            info: {\r\n               id: '16478080',\r\n               name: '荆辞雪',\r\n               college: '信息与计算机工程学院',\r\n            },\r\n            articleMenu: [\r\n               '宿管简介', '综合', '例检', '抽检', '晚归', '宿舍考评', '宿管公告', '下载'\r\n            ],\r\n            article: [\r\n               {\r\n                  date: '2018年6月11日',\r\n                  title: '信息与计算机工程学院抽检卫生结果通报',\r\n                  desc: '2018年6月11日周一下午16点30分学院抽检，本次抽检检查结果共计53个好寝、0个差寝。',\r\n                  post: 'posted @ 2018-6-13 21:17 宿管委员会寝工部'\r\n               },\r\n               {\r\n                  date: '2018年6月13日',\r\n                  title: '信息与计算机工程学院例检卫生结果通报',\r\n                  desc: '2018年6月13日周三下午16点30分学院例检，本次例检检查结果共计59个好寝、0个差寝。',\r\n                  post: 'posted @ 2018-6-13 21:20 宿管委员会寝工部'\r\n               },\r\n               {\r\n                  date: '2018年6月20日',\r\n                  title: '信息与计算机工程学院例检卫生结果通报',\r\n                  desc: '2018年6月20日周三下午16点30分学院例检，本次例检检查结果共计58个好寝，0个差寝。',\r\n                  post: 'posted @ 2018-6-20 21:06 宿管委员会寝工部'\r\n               },\r\n               {\r\n             